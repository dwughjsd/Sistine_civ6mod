		</Update>
		<Update>
			<Where StrategyType="VICTORY_STRATEGY_MILITARY_VICTORY" ConditionFunction="Leads Military"/>
			<Set ThresholdValue="10" />
		</Update>
		<Delete StrategyType="VICTORY_STRATEGY_CULTURAL_VICTORY" ConditionFunction="Tourists Taken" />
